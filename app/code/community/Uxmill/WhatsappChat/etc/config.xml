<config>
    <modules>
        <Uxmill_WhatsappChat>
            <version>1.0.2</version>
        </Uxmill_WhatsappChat>
    </modules>
    <global>
        <helpers>
            <whatsappchat>
                <class>Uxmill_WhatsappChat_Helper</class>
            </whatsappchat>
        </helpers>
        <blocks>
            <whatsappchat>
                <class>Uxmill_WhatsappChat_Block</class>
            </whatsappchat>
        </blocks>
        <models>
            <whatsappchat>
                <class>Uxmill_WhatsappChat_Model</class>
            </whatsappchat>
        </models>
    </global>
    <frontend>
        <layout>
            <updates>
                <whatsappchat>
                    <file>whatsappchat.xml</file>
                </whatsappchat>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <layout>
            <updates>
                <whatsappchat>
                    <file>whatsappchat.xml</file>
                </whatsappchat>
            </updates>
        </layout>
    </adminhtml>
</config>

