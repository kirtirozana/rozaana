<config>
    <acl>
        <resources>
            <all>
                <title>Allow Everything</title>
            </all>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <whatsappchat module="whatsappchat">
                                        <title>Uxmill Whatsapp Contact</title>
                                    </whatsappchat>
                                </children>
                            </config>
                        </children>
                    </system>
                    </children>
            </admin>
        </resources>
    </acl>
</config>
